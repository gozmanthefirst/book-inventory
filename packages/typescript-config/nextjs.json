{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "target": "ES2017",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "lib": ["DOM", "DOM.iterable", "ESNext"],
    "allowJs": true,
    "noEmit": true,
    "jsx": "preserve",
    "incremental": true
  }
}
